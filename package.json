{
  "name": "league-page",
  "version": "2.5.1",
  "homepage": "https://github.com/davezer/irvingLeague",
  "repository": {
    "type": "git",
    "url": "git://github.com/davezer/irvingLeague.git"
  },
  "author": {
    "name": "Dave Oliverio",
    "email": "daveoliverio@gmail.com",
    "url": "http://www.daveoliverio.dev/"
  },
  "license": "MIT",
  "engines": {
    "node": ">=v20.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "ingest": "dotenv -e .env -- node scripts/embed-upsert.js",
    "crawl": "dotenv -e .env -- node scripts/crawl-icl.js",
    "sync:sleeper": "dotenv -e .env -- node scripts/sync-sleeper.js",
    "test:db": "node scripts/test-db.js",
    "dev": "vite dev",
    "build": "vite build",
    "build-docker": "DOCKER_BUILD=true vite build --verbose",
    "docker-build": "docker build . -t $USER/league-page:latest",
    "docker-run": "npm run docker-build && docker run -p 3000:3000 $USER/league-page:latest",
    "preview": "vite preview",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "type": "module",
  "dependencies": {
    "@supabase/supabase-js": "^2.75.0",
    "@vercel/analytics": "^1.4.1",
    "analytics": "github:vercel/analytics",
    "contentful": "^10.6.7",
    "contentful-management": "^10.46.4",
    "cookie": "^0.7.0",
    "dompurify": "^3.3.0",
    "fast-xml-parser": "^4.2.5",
    "fuzzyjs": "^5.0.1",
    "googleapis": "^154.0.0",
    "html-to-text": "^9.0.5",
    "jquery": "^3.7.1",
    "jsdom": "^27.0.0",
    "marked": "^16.4.0",
    "openai": "^6.3.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@smui/button": "^8.0.0-beta.3",
    "@smui/data-table": "^8.0.0-beta.3",
    "@smui/dialog": "^8.0.0-beta.3",
    "@smui/drawer": "^8.0.0-beta.3",
    "@smui/icon-button": "^8.0.0-beta.3",
    "@smui/linear-progress": "^8.0.0-beta.3",
    "@smui/list": "^8.0.0-beta.3",
    "@smui/paper": "^8.0.0-beta.3",
    "@smui/tab": "^8.0.0-beta.3",
    "@smui/tab-bar": "^8.0.0-beta.3",
    "@smui/textfield": "^8.0.0-beta.3",
    "@sveltejs/adapter-netlify": "^5.1.1",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/adapter-vercel": "^5.5.3",
    "@sveltejs/kit": "^2.16.1",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "html-to-text": "^9.0.5",
    "jsdom": "^24.0.0",
    "playwright": "^1.48.2",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "smui-theme": "^8.0.0-beta.3",
    "svelte": "^5.19.2",
    "unplugin-icons": "^22.1.0",
    "zod": "^3.23.8"
  }
}
